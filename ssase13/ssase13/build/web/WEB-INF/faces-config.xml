<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>friendProfile</managed-bean-name>
        <managed-bean-class>com.jsflogin.friendProfile</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean> 
    
    <managed-bean>
        <managed-bean-name>NonFriendProfile</managed-bean-name>
        <managed-bean-class>com.jsflogin.NonFriendProfile</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>SearchByName</managed-bean-name>
        <managed-bean-class>com.jsflogin.SearchByName</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>JsfLogin</managed-bean-name>
        <managed-bean-class>com.jsflogin.JsfLogin</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>Register</managed-bean-name>
        <managed-bean-class>com.jsflogin.Register</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PartnersData</managed-bean-name>
        <managed-bean-class>com.jsflogin.PartnersData</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>interestPage</managed-bean-name>
        <managed-bean-class>com.jsflogin.interestPage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MyProfile</managed-bean-name>
        <managed-bean-class>com.jsflogin.MyProfile</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/searchByName.jsp</from-view-id>
        <navigation-case>
            <from-action>#{SearchByName.goToProfile}</from-action>
            <from-outcome>myProfile</from-outcome>
            <to-view-id>/myProfile.jsp</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-action>#{SearchByName.goToProfile}</from-action>
            <from-outcome>friendProfile</from-outcome>
            <to-view-id>/friendProfile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{SearchByName.goToProfile}</from-action>
            <from-outcome>nonFriendProfile</from-outcome>
            <to-view-id>/nonFriendProfile.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/register.jsp</from-view-id>
        <navigation-case>
            <from-action>#{Register.registerUser}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-action>#{Register.registerUser}</from-action>
            <from-outcome>fail</from-outcome>
            <to-view-id>/Fail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/myProfile.jsp</from-view-id>
        <navigation-case>
            <from-action>#{MyProfile.goToInterestProfile}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/interestPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/myProfile.jsp</from-view-id>
        <navigation-case>
            <from-action>#{MyProfile.goToFriendProfile}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/friendProfile.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
        <from-view-id>/myProfile.jsp</from-view-id>
        <navigation-case>
            <from-action>#{MyProfile.searchForUserAndNavigate}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/searchByName.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/interestPage.jsp</from-view-id>
        <navigation-case>
            <from-action>#{interestPage.goToProfile}</from-action>
            <from-outcome>myProfile</from-outcome>
            <to-view-id>/myProfile.jsp</to-view-id> 
        </navigation-case>
        <navigation-case>
            <from-action>#{interestPage.goToProfile}</from-action>
            <from-outcome>friendProfile</from-outcome>
            <to-view-id>/friendProfile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{interestPage.goToProfile}</from-action>
            <from-outcome>nonFriendProfile</from-outcome>
            <to-view-id>/nonFriendProfile.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/friendProfile.jsp</from-view-id>
        <navigation-case>
            <from-action>#{friendProfile.goToProfile}</from-action>
            <from-outcome>myProfile</from-outcome>
            <to-view-id>/myProfile.jsp</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-action>#{friendProfile.goToProfile}</from-action>
            <from-outcome>friendProfile</from-outcome>
            <to-view-id>/friendProfile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{friendProfile.goToProfile}</from-action>
            <from-outcome>nonFriendProfile</from-outcome>
            <to-view-id>/nonFriendProfile.jsp</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-action>#{friendProfile.goToInterestProfile}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/interestPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/login.jsp</from-view-id>
            <navigation-case>
                <from-action>#{JsfLogin.CheckValidUser}</from-action>
                <from-outcome>success</from-outcome>
                <to-view-id>/myProfile.jsp</to-view-id>
                
            </navigation-case>
            
            <navigation-case>
                <from-action>#{JsfLogin.navigatePartners}</from-action>
                <from-outcome>success</from-outcome>
                <to-view-id>/partnersData.jsp</to-view-id>
                
            </navigation-case>
            
        <navigation-case>
            <from-action>#{JsfLogin.CheckValidUser}</from-action>
            <from-outcome>fail</from-outcome>
            <to-view-id>/fail.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{JsfLogin.register}</from-action>
            <from-outcome>Register</from-outcome>
            <to-view-id>/register.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
